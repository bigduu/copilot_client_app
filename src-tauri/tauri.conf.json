{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AgentChat",
  "version": "0.2.0",
  "identifier": "com.agent.chat",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "AgentChat",
        "width": 1200,
        "height": 800,
        "decorations": true,
        "transparent": false,
        "center": true,
        "visible": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "label": "main"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
